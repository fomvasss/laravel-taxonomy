{
    "name": "fomvasss/laravel-taxonomy",
    "description": "Management of taxonomy in Laravel projects",
    "type": "composer-package",
    "keywords": [
        "laravel-taxonomy",
        "taxonomy",
        "terms",
        "categories",
        "tree",
        "laravel"
    ],
    "homepage": "https://bitbucket.org/fomvasss/laravel-taxonomy",
    "authors": [
        {
            "name": "Vasyl Fomin",
            "email": "fomvasss@gmail.com",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "require": {
        "php" : ">=7.0.0",
        "illuminate/support": "5.5.*|5.6.*|5.7.*|5.8.*",
        "illuminate/database": "5.5.*|5.6.*|5.7.*|5.8.*",
        "kalnoy/nestedset": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Fomvasss\\Taxonomy\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fomvasss\\Taxonomy\\TaxonomyServiceProvider"
            ]
        }
    }
}
